<?xml version="1.0" encoding="utf-8" ?>
<update comment="Move FBP outputs to base">
  <!--Remove FBP options table-->
  <action code="Exec" condition="TableExists" criteria="burnP3PlusFireSTARR_OutputOptionSpatial">
    <item> DROP TABLE burnP3PlusFireSTARR_OutputOptionSpatial </item>
  </action>

  <!--Remove tables for storing FBP outputs-->
  <action code="Exec" condition="TableExists" criteria="burnP3PlusFireSTARR_OutputRateOfSpreadMap">
    <item> DROP TABLE burnP3PlusFireSTARR_OutputRateOfSpreadMap </item>
  </action>

  <action code="Exec" condition="TableExists" criteria="burnP3PlusFireSTARR_OutputFireIntensityMap">
    <item> DROP TABLE burnP3PlusFireSTARR_OutputFireIntensityMap </item>
  </action>

  <action code="Exec" condition="TableExists" criteria="burnP3PlusFireSTARR_OutputSpreadDirectionMap">
    <item> DROP TABLE burnP3PlusFireSTARR_OutputSpreadDirectionMap </item>
  </action>

</update>